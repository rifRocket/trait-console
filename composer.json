{
    "name": "rifrocket/trait-console",
    "description": "Console command to create Traits for the Laravel",
    "keywords": [
        "rifrocket",
        "trait-console"
    ],
    "homepage": "https://github.com/rifrocket/trait-console",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mohammad Arif",
            "email": "developer.tech.dev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/console": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|~6.0|7.0|^8.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|~6.0|7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8.6",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Rifrocket\\TraitConsole\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rifrocket\\TraitConsole\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rifrocket\\TraitConsole\\TraitConsoleServiceProvider"
            ]
        }
    }
}
